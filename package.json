{
    "name": "university-bn",
    "version": "0.2.0",
    "description": "A backend for University management system built with Express and TypeScript.",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon",
        "build": "rimraf dist/ && tsc",
        "start:prod": "node dist/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "ts-node src/index.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:apply": "npm run typeorm -- -d src/configs/database.ts migration:run",
        "migration:revert": "npm run typeorm -- -d src/configs/database.ts migration:revert",
        "migration:generate": "ts-node ./src/utils/generateMigrations.ts",
        "seed": "ts-node -r tsconfig-paths/register src/utils/generateSeed.ts",
        "lint": "eslint src --ext .ts --fix",
        "format": "prettier --write \"src/**/*.{js,json,ts}\"",
        "type-check": "tsc --noEmit",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "express": "^4.18.3",
        "express-async-handler": "^1.2.0",
        "express-rate-limit": "^7.2.0",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.9.2",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.20",
        "typeorm-extension": "^3.5.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/bcrypt": "^5.0.2",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/faker": "^6.6.9",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^16.11.10",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.2",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "ts-node": "10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.5.2"
    }
}
